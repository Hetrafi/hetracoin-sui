{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "lib": ["ES2019", "DOM"]
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
} 